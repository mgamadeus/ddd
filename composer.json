{
  "name": "mgamadeus/ddd",
  "description": "A DDD Entity Framework on top of symfony and doctrine",
  "minimum-stability": "stable",
  "license": "MIT",
  "version": "1.4.8",
  "authors": [
    {
      "name": "Marius-Eugen Gerdan",
      "email": "marius@gerdan.net"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-gd": "*",
    "ext-iconv": "*",
    "ext-libxml": "*",
    "ext-openssl": "*",
    "ext-zlib": "*",
    "ext-imagick": "*",
    "beberlei/doctrineextensions": "^1.3",
    "doctrine/doctrine-bundle": "^2.8",
    "doctrine/inflector": "2.*",
    "doctrine/orm": "^2.13",
    "guzzlehttp/guzzle": "^7.5",
    "jms/parser-lib": "^1.0",
    "mockery/mockery": "^1.5",
    "phpunit/phpunit": "^9.5",
    "predis/predis": "2.0.0",
    "symfony/amqp-messenger": "*",
    "symfony/console": "*",
    "symfony/expression-language": "*",
    "symfony/flex": "*",
    "symfony/framework-bundle": "*",
    "symfony/http-client": "*",
    "symfony/messenger": "*",
    "symfony/monolog-bundle": "*",
    "symfony/process": "*",
    "symfony/proxy-manager-bridge": "*",
    "symfony/redis-messenger": "*",
    "symfony/runtime": "*",
    "symfony/security-bundle": "*",
    "symfony/translation": "*",
    "symfony/validator": "*",
    "symfony/dotenv": "*",
    "symfony/yaml": "*",
    "twig/twig": "*",
    "dragonmantank/cron-expression": "^3.3",
    "firebase/php-jwt": "^6.10",
    "dompdf/dompdf": "^2.0",
    "giggsey/libphonenumber-for-php": "^8.13"
  },
  "autoload": {
    "psr-4": {
      "DDD\\": "src/"
    },
    "files": [
      "src/Infrastructure/Helpers/helpers_bootstrap.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": false,
      "symfony/runtime": false
    }
  }
}
