{
  "name": "mgamadeus/ddd",
  "description": "A DDD Entity Framework on top of symfony and doctrine",
  "minimum-stability": "stable",
  "license": "MIT",
  "version": "1.1.0",
  "authors": [
    {
      "name": "Marius-Eugen Gerdan",
      "email": "marius@gerdan.net"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-gd": "*",
    "ext-iconv": "*",
    "ext-libxml": "*",
    "ext-openssl": "*",
    "ext-zlib": "*",
    "ext-imagick": "*",
    "beberlei/doctrineextensions": "^1.3",
    "doctrine/doctrine-bundle": "^2.8",
    "doctrine/inflector": "2.0.6",
    "doctrine/orm": "^2.13",
    "guzzlehttp/guzzle": "^7.5",
    "jms/parser-lib": "^1.0",
    "mockery/mockery": "^1.5",
    "phpunit/phpunit": "^9.5",
    "predis/predis": "2.0.0",
    "symfony/amqp-messenger": "6.0.*",
    "symfony/console": "6.0.*",
    "symfony/expression-language": "v6.0.*",
    "symfony/flex": "^2.2",
    "symfony/framework-bundle": "6.0.*",
    "symfony/http-client": "v6.0.*",
    "symfony/messenger": "6.0.*",
    "symfony/monolog-bundle": "^3.8",
    "symfony/process": "6.0.*",
    "symfony/proxy-manager-bridge": "6.0.*",
    "symfony/redis-messenger": "6.0.*",
    "symfony/runtime": "6.0.*",
    "symfony/security-bundle": "6.0.*",
    "symfony/translation": "v6.0.*",
    "symfony/validator": "6.0.*",
    "symfony/yaml": "6.0.*",
    "twig/twig": "^3.8",
    "dragonmantank/cron-expression": "^3.3",
    "firebase/php-jwt": "^6.10",
    "dompdf/dompdf": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "DDD\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": false,
      "symfony/runtime": false
    }
  }
}
