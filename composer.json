{
  "name": "mgamadeus/ddd",
  "description": "A DDD Entity Framework on top of symfony and doctrine",
  "minimum-stability": "stable",
  "license": "MIT",
  "version": "2.8.29",
  "authors": [
    {
      "name": "Marius-Eugen Gerdan",
      "email": "marius@gerdan.net"
    }
  ],
  "require": {
    "php": ">=8.3",
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-gd": "*",
    "ext-iconv": "*",
    "ext-libxml": "*",
    "ext-openssl": "*",
    "ext-zlib": "*",
    "ext-imagick": "*",
    "beberlei/doctrineextensions": "^1.3",
    "doctrine/doctrine-bundle": "^2.8",
    "doctrine/inflector": "2.*",
    "doctrine/orm": "^2.13",
    "guzzlehttp/guzzle": "^7.5",
    "jms/parser-lib": "^1.0",
    "mockery/mockery": "^1.5",
    "phpunit/phpunit": "^9.5",
    "predis/predis": "2.0.0",

    "symfony/amqp-messenger": "7.3.*",
    "symfony/console": "7.3.*",
    "symfony/expression-language": "7.3.*",
    "symfony/framework-bundle": "7.3.*",
    "symfony/http-client": "7.3.*",
    "symfony/messenger": "7.3.*",
    "symfony/dependency-injection": "7.3.*",
    "symfony/var-exporter": "7.3.*",
    "symfony/monolog-bundle": "^4.0",
    "symfony/proxy-manager-bridge": ">=6.4 <7.0",
    "symfony/process": "7.3.*",
    "symfony/redis-messenger": "7.3.*",
    "symfony/runtime": "7.3.*",
    "symfony/security-bundle": "7.3.*",
    "symfony/translation": "7.3.*",
    "symfony/validator": "7.3.*",
    "symfony/dotenv": "7.3.*",
    "symfony/yaml": "7.3.*",
    "symfony/routing": "7.3.*",

    "twig/twig": "^3.0",

    "dragonmantank/cron-expression": "^3.3",
    "firebase/php-jwt": "^6.10",
    "dompdf/dompdf": "^2.0",
    "giggsey/libphonenumber-for-php": "^8.13",
    "brick/geo-doctrine": "^0.3.1",
    "kamermans/guzzle-oauth2-subscriber": "^1.1",
    "phpoffice/phpspreadsheet": "^4.2"
  },
  "autoload": {
    "psr-4": {
      "DDD\\": "src/"
    },
    "files": [
      "src/Infrastructure/Helpers/helpers_bootstrap.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "symfony/runtime": false
    }
  }
}
